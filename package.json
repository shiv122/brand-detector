{
  "name": "detector-backend",
  "version": "1.0.0",
  "description": "Logo Detection API with GPU acceleration for Apple Silicon",
  "main": "main.py",
  "scripts": {
    "start": "uv run python main.py",
    "dev": "uv run python main.py --dev",
    "test": "uv run python test_gpu.py",
    "install": "uv sync",
    "install:dev": "uv sync --extra dev",
    "setup": "chmod +x start.sh && ./start.sh --help",
